{
  "name": "react-easy-pie-chart",
  "version": "0.0.0",
  "description": "Barebone React pie/arc chart",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "files": [
    "es",
    "lib"
  ],
  "scripts": {
    "test": "jest",
    "clean": "rm -rf lib es",
    "compile": "npm run clean && npm run compile:es && npm run compile:commonjs",
    "compile:es": "babel -d es/ index.js",
    "compile:commonjs": "NODE_ENV=commonjs babel -d lib/ index.js",
    "preversion": "npm test",
    "version": "git add package.json",
    "postversion": "git push && git push --tags",
    "prepublish": "npm run compile"
  },
  "keywords": [
    "react",
    "pie",
    "arc",
    "chart",
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toomuchdesign/react-easy-pie-chart.git"
  },
  "author": "Andrea Carraro <me@andreacarraro.it>",
  "license": "MIT",
  "peerDependencies": {
    "react": "^15.0.0",
    "react-dom": "^15.0.0"
  },
  "dependencies": {
    "svg-arc-corners": "^1.0.4"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-jest": "^19.0.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-register": "^6.23.0",
    "jest": "^19.0.2"
  }
}
